<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.emobilis.myshop.Main2">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="500px"
        android:layout_height="500px"
        android:layout_above="@+id/imageButton11"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="23dp"
        app:srcCompat="@drawable/auto" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="500px"
        android:layout_height="500px"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageButton"
        app:srcCompat="@drawable/carparts" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="500px"
        android:layout_height="500px"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="71dp"
        app:srcCompat="@drawable/spareparts" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="500px"
        android:layout_height="500px"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageButton10"
        app:srcCompat="@drawable/usedparts" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="37dp"
        android:layout_marginTop="33dp"
        android:text="Trigga Spare Parts" />

</RelativeLayout>
